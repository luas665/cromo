while true
    idade = inpnum("Digite sua idade: ", "\nDigite apenas numeros")

    if idade >= 18
        print("\nVocê é maior de idade!")
    else if idade >= 16
        print("\nVocê é um jovem!")
    else if idade >= 12
        print("\nVocê é um adolescente!")
    else 
        print("\nVocê é uma criança!")
    end
end


